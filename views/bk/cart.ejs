<tr><td width="5">
<% if(typeof mer[i][0]!=="undefined" ) {%>
<img src="<%= mer[i][0].img%>">
</td><td>
name:<%= mer[i][0].item%><br>
price:<%= mer[i][0].pri%><br>
sku:<%= tmp[i].sku%><br>
uni:<%= tmp[i].uni%><br>
<%}else  { %>
<img src="<%= mer[i].img%>">
</td><td>
name:<%= mer[i].item%><br>
price:<%= mer[i].pri%><br>
sku:<%= tmp[i].sku%><br>
uni:<%= tmp[i].uni%><br>

<%} %>
</td></tr>
</table>
</div>
<%}else{ %>
<div class="well col-lg-6 col-sm-12">
<table class="table" border="1">
<tr><td width="5">

<% if(typeof mer[i][0]!=="undefined" ) {%>
<img src="<%= mer[i][0].img%>">
</td><td>
name:<%= mer[i][0].item%><br>
price:<%= mer[i][0].pri%><br>
sku:<%= tmp[i].sku%><br>
uni:<%= tmp[i].uni%><br>
<%}else  { %>
<img src="<%= mer[i].img%>">
</td><td>
name:<%= mer[i].item%><br>
price:<%= mer[i].pri%><br>
sku:<%= tmp[i].sku%><br>
uni:<%= tmp[i].uni%><br>

<%} %>
</td></tr>
</table>
</div>
<%} %>
<%} %>
<br>

<div class="well col-md-12" style="background-color:cornsilk"><h2>sum:<%= sum.toLocaleString() %></h2></div>

</div><!-- col -->

<form action="/pay" method="post">
<input type="submit" value="pay">
</form>

</div><!-- row -->

</div><!-- cont -->

<% include par/2side %>

</div><!-- row -->
</div><!-- cont -->


</body>

