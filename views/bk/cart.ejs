<% include par/head %>
<body>
<% include par/nav %>
<div class="container-fluid">
<div class="row">

<div class="well col-lg-2 col-sm-12">
<% if(typeof usr!=="undefined") {%> 

<div class="label label-info">
welcome: 
<%= usr%>
</div>
<%}else{%>
<div class="label label-warning">
not logged in
</div>
<%} %>

</div>
<div style="background-color:khaki" class="well col-lg-8 col-md-12">
<div class="container-fluid">
<div class="row">

<div class="well col-lg-12 col-sm-12">
len:<%= tmp.length %>
<% if (typeof sku!=="undefined" && typeof uni!="undefined"){%>
<%= sku %>
<%= uni %>
<%} %>
</div>

<% for(var i=0;i<tmp.length;i++){ %>
<% if(i%2==0){ %>
<div class="well col-lg-6 col-sm-12">
